generator client {
  provider  = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/gym?sslmode=disable"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Chester {
  id Int @unique
  name String?
  img String?
}

model Back {
  id Int @unique
  name String?
  img String?
}

model Leg {
  id Int @unique
  name String?
  img String?
}

model Biceps {
  id Int @unique
  name String?
  img String?
}
